# HELP num_events Total number of events
# TYPE num_events gauge
num_events{status="confirmed"}   <%= @events[:confirmed] %>   <%= @timestamp %>
num_events{status="unconfirmed"} <%= @events[:unconfirmed] %> <%= @timestamp %>

# HELP num_entries Total number of entries
# TYPE num_entries gauge
num_entries{status="unconfirmed"}                  <%= @entries[:unconfirmed] %>        <%= @timestamp %>
num_entries{type="offer", direction="way_there"}   <%= @entries[:offers_way_there] %>   <%= @timestamp %>
num_entries{type="offer", direction="way_back"}    <%= @entries[:offers_way_back] %>    <%= @timestamp %>

# HELP num_altcha_solutions Total number of altcha solutions
# TYPE num_altcha_solutions gauge
num_altcha_solutions <%= @altcha_solutions %> <%= @timestamp %>
